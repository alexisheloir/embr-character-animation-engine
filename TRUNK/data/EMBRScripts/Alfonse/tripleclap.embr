    BEGIN POSITION_CONSTRAINT
      BODY_GROUP:rarm
      TARGET:-0.02;-0.31;0.32
      JOINT:rhand
      OFFSET:0.0;0.0;0.0
    END
    BEGIN ORIENTATION_CONSTRAINT
      BODY_GROUP:rarm
      DIRECTION:-1.0;0.0;0.0
      NORMAL:Zaxis
      JOINT:rhand
    END
    BEGIN POSITION_CONSTRAINT
      BODY_GROUP:larm
      TARGET:0.03;-0.36;0.33
      JOINT:lhand
      OFFSET:0.0;0.0;0.0
    END
    BEGIN ORIENTATION_CONSTRAINT
