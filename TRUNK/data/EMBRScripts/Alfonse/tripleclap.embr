BEGIN K_POSE_SEQUENCE
  CHARACTER:Alfonse
  START:asap
  FADE_IN:200
  FADE_OUT:200
  BEGIN K_POSE            #  ---- Rest Pose  ----  #
    TIME_POINT:+200
    HOLD:500
    BEGIN POSITION_CONSTRAINT
      BODY_GROUP:rarm
      TARGET:-0.3;-0.03;0.02
      JOINT:rhand
      OFFSET:0.0;0.0;0.0
    END
    BEGIN POSITION_CONSTRAINT
      BODY_GROUP:larm
      TARGET:0.2;-0.03;0.02
      JOINT:lhand
      OFFSET:0.0;0.0;0.0
    END
  END
  BEGIN K_POSE         #  ---- First Pose hands touching ----   #
    TIME_POINT:+900
